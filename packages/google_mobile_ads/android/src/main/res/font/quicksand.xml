        android:fontStyle="normal"
        android:fontWeight="600"
        android:font="@font/quicksand_semibold"
        app:fontStyle="normal"
        app:fontWeight="600"
        app:font="@font/quicksand_semibold"/>
    <font
        android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/quicksand_bold"
